{
  "name": "rachio-ditch-sensor",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "NODE_ENV=development nodemon --exec babel-node ./src/server.js",
    "open:dist": "NODE_ENV=production pm2 start --no-daemon dist/server.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel src -d dist",
    "flow": "flow",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Nathan Fiscus",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.2.2",
    "method-override": "^2.3.10",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rachio": "^0.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "6.23.0",
    "chai": "3.5.0",
    "cross-env": "5.0.5",
    "eslint": "4.18.2",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-flowtype": "2.32.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "3.1.0",
    "flow-bin": "0.43.1",
    "glob": "7.1.1",
    "istanbul": "0.4.5",
    "jest": "19.0.2",
    "js-yaml": "^3.13.1",
    "minimist": "1.2.3",
    "nodemon": "^1.19.1",
    "npm-run-all": "4.0.2",
    "pretty-format": "21.2.1",
    "request": "^2.88.0",
    "request-promise": "4.2.2",
    "rimraf": "2.6.1",
